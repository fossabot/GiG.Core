{
  "Logging": {
    "MinimumLevel": "Information",
    "Sinks": {
      "Console": {
        "IsEnabled": true
      }
    }
  },
  "Orleans": {
    "MembershipProvider": {
      "Name": "Consul"
    },
    "Cluster": {
      "ClusterId": "dev",
      "ServiceId": "sample"
    },
    "Dashboard": {
      "IsEnabled": true
    },
    "StorageProviders": {
      "SampleDb": {
        "ConnectionString": "Host=localhost;Username=postgres;Password=postgres;Database=sample"
      }
    },
    "HealthChecks": {
      "Port": 7777,
      "HealthCheckUrl" :  "/orleanshealth"
    }
  }
}